<p id="notice"><%= notice %></p>

<h1>Movies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movies.each do |movie| %>
      <div class="col-md-3">
        <%= link_to movie_path(:id => movie.id) do %>
    			<%= movie.title %>
  		  <% end %>
      </div>
    <% end %>
  </tbody>
</table>

<br>
